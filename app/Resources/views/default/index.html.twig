{# src/Resources/views/admin/q.tag.html.twig #} 
{% extends 'default/init.html.twig' %} 


{% block body %}
	<div id="" class="container"> 

		<div id="panel" class="panel panel-default">
			<div class="panel-heading">
            	<h3 class="panel-title">Da ist die ganze Liste von allen Quiz</h3>
            	
            </div>
			<div class="panel-body">
				<div class="panel-scroll">
				<table class="table table-bordered" fixed-header>
					<thead>
                        <tr>
                          <th style="width:50px">#</th>
                          <th style="width:250px">Titel</th>
                        </tr>
                    </thead>
                    {% for e in quizs %}
                    	<tr>
                    		<td style="width:50px">
                    			<a class="" href="javascript:alert('Help yourself!')" style="">{{ e.id }}</a>
                    		</td>
                    		<td style="width:250px">
                    			<a class="" href="javascript:alert('Help yourself!')" style="">{{ e.title }}</a>
                    		</td>
                		</tr>
                	{% endfor %}
				</table>
            	</div>
            	<br/>
        	
        	</div>
		</div>
		
	</div>
{% endblock %} 